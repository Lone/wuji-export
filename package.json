{
  "name": "fix-padding-export",
  "description": "导出标注去掉字padding实现品质还原提升开发效率；基于utom&jebbs开发。",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=57.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lone/sketch-meaxure.git"
  },
  "skpm": {
    "name": "无敌弹簧转换器",
    "manifest": "src/manifest.json",
    "homepage": "https://github.com/qjebbs",
    "appcast_": "appcast.xml",
    "main": "fix-padding-export.sketchplugin",
    "assets": [
      "resources/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "export DEBUG=1; skpm-build --watch",
    "start": "export DEBUG=1; skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0",
    "@types/sketch": "git+https://github.com/qjebbs/sketch.d.ts.git",
    "skpm": "^1.3.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "author": {
    "name": "陈国良、刘东海",
    "email": "chenguoliang@baidu.com",
    "url": ""
  },
  "contributors":[{
      "name": "utom & jebbs",
      "email": "qjebbs@gmail.com"
  },{
    "name": "Lone",
    "email": "lone.chain@qq.com"
}],
  "dependencies": {
    "@skpm/path": "^0.1.4"
  }
}
